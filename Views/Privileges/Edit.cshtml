@model tpeapp.Models.PrivilegesModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>PrivilegesModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PrivilegesId" />
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPublisher" /> @Html.DisplayNameFor(model => model.IsPublisher)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPioneer" /> @Html.DisplayNameFor(model => model.IsPioneer)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsMinisterialServant" /> @Html.DisplayNameFor(model => model.IsMinisterialServant)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsElder" /> @Html.DisplayNameFor(model => model.IsElder)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPioneerAndMinisterialServant" /> @Html.DisplayNameFor(model => model.IsPioneerAndMinisterialServant)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPioneerAndElder" /> @Html.DisplayNameFor(model => model.IsPioneerAndElder)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

