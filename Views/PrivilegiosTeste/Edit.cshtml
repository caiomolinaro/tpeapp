@model tpeapp.Models.Privilegios

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Privilegios</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PrivilegiosId" />
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPublicador" /> @Html.DisplayNameFor(model => model.IsPublicador)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPioneiro" /> @Html.DisplayNameFor(model => model.IsPioneiro)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsServo" /> @Html.DisplayNameFor(model => model.IsServo)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsAnciao" /> @Html.DisplayNameFor(model => model.IsAnciao)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPioneiroAndServo" /> @Html.DisplayNameFor(model => model.IsPioneiroAndServo)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsPioneroAndAnciao" /> @Html.DisplayNameFor(model => model.IsPioneroAndAnciao)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

